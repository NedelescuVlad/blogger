<p id="notice"><%= notice %></p>

<h1>Listing Authors</h1>

<table>
  <tbody>
     <tr>
       <td>Userame</td>
       <td>Email</td>
     </tr>
    <% @authors.each do |author| %>
      <tr>
        <td><%= author.username %></td>
        <td><%= author.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<p> <%= link_to 'New Author', new_author_path %> </p>
<p> <%= link_to 'Back to Articles', articles_path %> </p>
